{
  "version": 3,
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)\.(html|css|js|png|jpg|jpeg|gif|svg|txt|json)$",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
